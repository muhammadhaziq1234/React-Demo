  "dependencies": {
    "axios": "^1.2.0",
    "bootstrap": "^5.2.3",
    "msw": "^0.49.1",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.3",
